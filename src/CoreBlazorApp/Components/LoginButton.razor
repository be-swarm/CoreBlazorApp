@namespace BeSwarm.CoreBlazorApp.Components
<div style="height:50px;">
   
    @if (!Session.SessionIsActive)
    {
        <MudButton Variant="Variant.Filled" Color="Color.Primary" @onclick="Login">Login</MudButton>
	}
    else
    {
        <MudButton Variant="Variant.Filled" Color="Color.Primary" @onclick="Logout">Logout</MudButton>
    }
 
</div>
